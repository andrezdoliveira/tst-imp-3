COLUMN TABLE "MARM_LOG" (
    MANDT VARCHAR2 (9) Not Null,
    MATNR VARCHAR2 (54) Not Null,
    MEINH VARCHAR2 (9) Not Null,
    UMREZ NUMBER (5) Not Null,
    UMREN NUMBER (5) Not Null,
    CDC VARCHAR (10) Not Null, 
/*    UUIDLOG VARCHAR2 (30) Null,  */
    TIME_STAMP TIMESTAMP Null,
        Primary Key (MANDT,MATNR,MEINH)
)