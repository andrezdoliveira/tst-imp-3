COLUMN TABLE "KNVL_LOG" (
    MANDT VARCHAR2 (9) Not Null,
    KUNNR VARCHAR2 (30) Not Null,
    ALAND VARCHAR2 (9) Not Null,
    TATYP VARCHAR2 (12) Not Null,
    LICNR VARCHAR2 (45) Not Null,
    DATAB VARCHAR2 (24) Null,
    DATBI VARCHAR2 (24) Null,
    BELIC VARCHAR2 (3) Null,
    --Campos extra de tablas log
    CDC VARCHAR (10) Not Null,UUID VARCHAR2 (100) Not Null,UUIDLOG VARCHAR2 (100) Null,TIME_STAMP VARCHAR2 (50) Not Null,
    Primary Key (MANDT, KUNNR, ALAND, TATYP, LICNR, CDC) 
)