COLUMN TABLE "LFB1_LOG" (
    MANDT VARCHAR2 (9) Not Null,
    LIFNR VARCHAR2 (30) Not Null,
    BUKRS VARCHAR2 (12) Not Null,
    PERNR VARCHAR2 (24) Null,
    --Campos extra de tablas log
    CDC VARCHAR (10) Not Null,UUID VARCHAR2 (100) Not Null,UUIDLOG VARCHAR2 (100) Null,TIME_STAMP VARCHAR2 (50) Not Null,
    Primary Key (MANDT, LIFNR, BUKRS, CDC)
)