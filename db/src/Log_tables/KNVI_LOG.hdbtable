COLUMN TABLE "KNVI_LOG" (
    MANDT VARCHAR2 (9) Not Null,
    KUNNR VARCHAR2 (30) Not Null,
    ALAND VARCHAR2 (9) Not Null,
    TATYP VARCHAR2 (12) Not Null,
    TAXKD VARCHAR2 (3) Null,
    --Campos extra de tablas log
    CDC VARCHAR (10) Not Null,UUIDLOG VARCHAR2 (30) Null,TIME_STAMP VARCHAR2 (50) Not Null,
    Primary Key (MANDT, KUNNR, ALAND, TATYP, CDC) 
)